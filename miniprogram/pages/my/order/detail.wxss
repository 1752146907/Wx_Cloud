page{
  background: #f7f7f7;
}

.flow-delivery{
  display: flex;
  padding: 20rpx 20rpx 30rpx 20rpx;
  background: #fff url('data:image/png;base64,') bottom left repeat-x;
  background-size: 120rpx auto; 
}
.flow-delivery .detail-location{
  width: 60rpx;
  display: flex;
  align-items: center
}
.flow-delivery .detail-content{
  flex: 1
}
.flow-delivery .detail-content-title{
  font-size: 34rpx;
}
.flow-delivery .detail-content-title-name{
  
}
.flow-delivery .detail-content-title-phone{
  font-size: 30rpx;
  margin-left: 20rpx;
}
.flow-delivery .detail-content-describe{
  color: #999999;
  font-size: 30rpx;
  margin-top: 10rpx;
}
.flow-delivery .detail-arrow{
  width: 40rpx;
  display: flex;
  align-items: center
}

.hander{
  background: #ffffff;
  margin-top: 20rpx;
  padding: 20rpx;
  font-size: 32rpx;
}
.hander .hander-title{
  padding-bottom: 10rpx;
  margin-bottom: 10rpx;
  border-bottom: 1px solid #f7f7f7;
}
.hander .hander-body{
  color: #777;
  font-size: 26rpx; 
}
.hander .hander-body-item{
  line-height: 50rpx;
}
.hander .hander-body-item text{
  font-size: 24rpx; 
  padding: 2rpx 10rpx;
  border: 1px solid #777;
  border-radius: 10rpx;
  margin-left: 20rpx;
}        

.list{
  margin: 20rpx 0rpx;
  background: #ffffff;
} 
.list-box{
  display: flex;
  color: #333333;
  margin-bottom: 10rpx;
  padding: 20rpx 20rpx 0rpx 20rpx;
  border-bottom: 1px solid #f7f7f7
}
.list-box .list-left{
  width: 160rpx;
  padding: 10rpx;
  margin-right: 20rpx;
}
.list-box .list-left image{
  width: 160rpx;
  height: 160rpx;
}
.list-box .list-right{
  flex: 1
}
.list-box .list-right-title{
  height: 84rpx;
  line-height: 40rpx;
  color: #333333;
  font-size: 32rpx;
  overflow : hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;  
  -webkit-box-orient: vertical;
}
.list-box .list-right-text{
  font-size: 28rpx;
  color: #999999;
}
.list-box .list-right-sku{
  font-size: 28rpx;
  display: flex;
  margin-top: 10rpx;
}
.list-box .list-right-sku .cont{
  flex: 1;
  color: #F75148
}
.list-box .list-right-sku .small{
  flex: 1;
  text-align: right
}
.list-footer{
  display: flex;
  align-items: center;
  justify-content: flex-end;
  font-size: 32rpx; 
  padding: 10rpx 20rpx 20rpx 0rpx;
} 
.list-footer-money{
  color: #F75148
}


.flow-footer{
  position: fixed;
  bottom: 0;
  width: 100%;
  background: #fff;
  border-top: 1px solid #eee;
  z-index: 11; 
  text-align: center;
  display: flex
}
.flow-footer-price{
  flex: 1;
  font-size: 32rpx;
  line-height: 92rpx;
  color: #777; 
}
.flow-footer-price text{
  color: #F75148
}
.flow-footer-money{
  flex: 1;
  color: #fff !important;
  line-height: 92rpx; 
  font-size: 32rpx;  
  background: linear-gradient(to right, #F75148 0, #F75148 100%);
}